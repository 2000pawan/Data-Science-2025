   "source": [
    "import cv2 as cv\n",
    "import numpy as np\n",
    "img=cv.imread('00019.jpg')\n",
    "rows,cols=img.shape[:2]\n",
    "M=cv.getRotationMatrix2D((cols/2,rows/2),45,1)\n",
    "dst=cv.warpAffine(img,M,(cols,rows))\n",
    "cv.imshow('Rotated_img',dst)\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()"
   ]
